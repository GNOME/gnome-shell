#. i.e. "May 25, 14:30"
#: ../js/misc/util.js:209
#, no-c-format
msgid "%B %d, %H∶%M"
msgstr "%m月%d日, %H:%M"

#. Translators: this is the month name, day number, year
#. number followed by a time string in 24h format.
#. i.e. "May 25 2012, 14:30"
#: ../js/misc/util.js:215
#, no-c-format
msgid "%B %d %Y, %H∶%M"
msgstr "%Y年%m月%d日, %H:%M"

#. Translators: Time in 12h format
#: ../js/misc/util.js:220
msgid "%l∶%M %p"
msgstr "%p %-l:%M"

