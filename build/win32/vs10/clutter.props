if exist ..\..\..\clutter\config.h goto DONE_CONFIG_H
copy ..\..\..\clutter\config.h.win32 ..\..\..\clutter\config.h

:DONE_CONFIG_H
if "$(Configuration)" == "Release" goto DO_CLUTTER_CONFIG_WIN32
